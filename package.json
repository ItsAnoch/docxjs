  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItsAnoch/docxjs.git"
  },
  "bugs": {
    "url": "https://github.com/ItsAnoch/docxjs/issues"
  },
  "homepage": "https://github.com/ItsAnoch/docxjs#readme",
  "dependencies": {
    "jszip": ">=3.0.0",
    "omml2mathml": "^1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "diff": "^5.0.0",
    "jasmine-core": "^5.1.0",
    "karma": "^6.3.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.0.0",
    "rollup": "^4.9.1",
    "tslib": "^2.4.0",
    "typescript": "^5.0.3"
  },
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "build-prod": "rollup --config rollup.config.mjs --environment BUILD:production",
